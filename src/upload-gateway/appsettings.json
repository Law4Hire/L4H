{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Uploads": {
    "BasePath": "/data/uploads",
    "QuarantineSubdir": "quarantine",
    "MaxSizeMB": 25,
    "AllowedExtensions": [ "pdf", "docx", "doc", "png", "jpg", "jpeg", "tiff", "gif", "heic" ],
    "Token": {
      "SigningKey": "CHANGE_ME_DEV_ONLY",
      "TtlMinutes": 30
    }
  }
}